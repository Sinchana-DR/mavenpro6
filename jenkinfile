pipeline { 
    agent any 
    tools { 
          maven 'Maven 3.9.9' //Ensure name matches with configured  
    } 
    stages { 
        stage('Checkout') {  
            steps { 
                git branch: 'master', url: 
'https://github.com/Sinchana-DR/mavenpro6.git'  
            } 
    } 
     stage('Build') {  
            steps { 
                bat 'mvn clean package'  
            } 
      } 
     stage('Test') {  
            steps { 
                bat 'mvn test'  
            } 
      } 
     stage('Run Application') {  
            steps { 
                bat 'java -jar target/mavenpro6-0.0.1-SNAPSHOT.jar'  
            } 
      } 
    } 
}
